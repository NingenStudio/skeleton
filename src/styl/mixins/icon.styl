icon(args...)
  background-image url(args[0])
  background-position 50% 50%
  background-repeat no-repeat
  background-size contain
  border 0
  display inline-block
  font-size 0
  if(length(args) == 2)
    height args[1]
    width args[1]
  else if(length(args) == 3)
    height args[2]
    width args[1]
  else
    height 10px
    width 10px